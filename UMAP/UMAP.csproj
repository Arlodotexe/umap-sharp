<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>UMAP</RootNamespace>
    <AssemblyName>UMAP</AssemblyName>
    <Authors />
    <Company>Curiosity GmbH</Company>
    <Product />
    <Description>A C# implementation of "Uniform Manifold Approximation and Projection" (UMAP)</Description>
    <PackageLicenseExpression>MIT License</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/curiosity-ai/umap-csharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/curiosity-ai/umap-csharp</RepositoryUrl>
    <PackageTags>UMAP, kNN, ANN, approximate nearest neighbor</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Numerics.Tensors" Version="0.2.0-preview6.19303.8" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.6.0-preview5.19224.8" />
  </ItemGroup>

</Project>
